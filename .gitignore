-- Funci贸n para desactivar colisiones de otros jugadores
local function disableCollisionsForOtherPlayers()
    for _, player in pairs(Players:GetPlayers()) do
        -- Verificar que el jugador no sea el "speaker" (t煤) y que su personaje exista
        if player ~= speaker and player.Character then
            -- Recorrer todos los descendientes del personaje del jugador
            for _, descendant in pairs(player.Character:GetDescendants()) do
                -- Si el descendiente es una parte f铆sica (BasePart), desactivar la colisi贸n
                if descendant:IsA("BasePart") then
                    descendant.CanCollide = false
                end
            end
        end
    end
end

-- Funci贸n que se ejecuta cuando el personaje reaparece
local function onCharacterAdded(newCharacter)
    character = newCharacter -- Actualizar la variable del personaje
    -- Ejecutar la funci贸n cuando el personaje reaparece
    disableCollisionsForOtherPlayers()
    
    -- Conectar el evento para cuando el personaje muera
    local humanoid = character:WaitForChild("Humanoid")
    humanoid.Died:Connect(function()
        print("Personaje muerto, esperando el respawn...")
    end)
    
    -- Mantener la desactivaci贸n de colisiones para otros jugadores en cada frame
    RunService.Heartbeat:Connect(function()
        disableCollisionsForOtherPlayers() -- Reaplicar desactivaci贸n de colisiones constantemente
    end)
end

-- Conectar el evento CharacterAdded para cuando el personaje reaparezca
